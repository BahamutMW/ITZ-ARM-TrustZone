if {[have_include "power_on/qemu"]} {
	puts "\nNetperf running on Qemu is not supported.\n"
	exit
}

# network configuration
set use_nic_bridge      1

set use_usb_11          "no"
set use_usb_20          "no"
set use_usb_30          "yes"

source ${genode_dir}/repos/ports-foc/run/l4linux_netperf.inc
